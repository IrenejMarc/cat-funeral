<h1>Login</h1>

<%= form_for :login, url: login_path do |f| %>
    <p>
      <%= f.label :username %>
      <%= f.text_field :username %>

    <p>
      <%= f.label :password %>
      <%= f.password_field :password %>

    <p>
      <%= f.submit 'Login' %>
<% end %>

<%= link_to 'Register', register_path %>